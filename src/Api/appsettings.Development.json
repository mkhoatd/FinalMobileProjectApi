{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://0.0.0.0:5000"
      },
      "Http": {
        "Url": "http://0.0.0.0:5001"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "./certs/fullchain.pem",
        "KeyPath": "./certs/privkey.pem"
      }
    }
  }
}
